  <div class="col-xs-12 col-sm-6 col-sm-offset-3">
  {% if parameters.matchDay %}
    <h1 class="text-center">Voti giornata {{parameters.matchDay}}</h1>
  {% endif %}
  {% if parameters.playerMarks %}
    <div class="table-responsive">
      <table class="table table-hover table-bordered table-condensed">
        <thead>
          <tr>
            <th class="text-center">Ruolo</th>
            <th class="text-center">Nome</th>
            <th class="text-center">Voto</th>
          </tr>
        </thead>
        <tbody>
          {% for role,data in parameters.playerMarks if data %}
          <tr class="text-center">
            {% set role_new = role|split('-',3) %}
            {% if role_new[2] is defined %}
              <td>{{role_new[0]}} {{role_new[1]}} {{role_new[2]}}</td>
            {% elseif role_new[1] is defined %}
              <td>{{role_new[0]}} {{role_new[1]}}</td>
            {% else %}
              <td>{{role_new[0]}}</td>
            {% endif %}
            {% set name_new = data.name|split(' ', 2) %}
            {% if name_new[1] is defined %}
              <td>{{name_new[1]}}</td>
            {% else %}
              <td>{{name_new[0]}}</td>                
            {% endif %}
              <td>{% if data.mark is null or data.mark == 0 %} S.V {% else %} {{data.mark}} {% endif %}</td>
          </tr>
          {% endfor %}
        </tbody>
      </table>
    </div>
  {% endif %}
  </div>